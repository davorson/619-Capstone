sigsOut_2 = sigsOut

Irms_2 = sigsOut_2{1}.Values.Data;
d_Irms_2 = zeros(1, length(sigsOut_2{1}.Values.Data));

for n = 2:length(Irms_2)
    d_Irms_2(n) = Irms_2(n) - Irms_2(n - 1);
end

plot(d_Irms_2);
title('Processed Training Data (1st Derivative / Change in Data)');
xlabel('Sample');
ylabel('dI/dt (A/s)');
grid on;
